{

  "compilerOptions": {
    "target": "esnext",
    "module": "esnext",
    "baseUrl": "src",
    "moduleResolution": "bundler",
    "skipLibCheck": true,
    "alwaysStrict": true,
    "noImplicitAny": true,
    "noFallthroughCasesInSwitch": true,
    "newLine": "lf",
    "strictNullChecks": true,
    "strictFunctionTypes": true,
    "noEmit": true
  },
  "include": ["env.d.ts", "src/**/*", "src/**/*.vue", "src/**/*.json"]
}
